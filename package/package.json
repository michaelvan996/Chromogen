{
  "name": "chromogen",
  "version": "4.0.4",
  "description": "simple, interaction-driven Jest test generator for Recoil and React Hooks apps",
  "main": "build/index.js",
  "keywords": [
    "react",
    "recoil",
    "jest",
    "testing"
  ],
  "files": [
    "build"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc",
    "test": "jest --verbose --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "update": "tsc && npm --prefix ../demo-zustand-todo run update",
    "update-z": "tsc && npm --prefix ../../zustand/examples/demo run update",
    "symlink": "npm --prefix ../demo-zustand-todo run symlink"
  },
